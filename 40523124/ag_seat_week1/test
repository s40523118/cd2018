#test4
a=[36,25,9,]
print('3次成績: ', end='')
for grade in a:
    print(grade, end=' ')
print(end='\n')
average = sum(a)/len(a)
print('3次平均: ',end='')
print(average)
print('新成績: ', end='')
for grade in range(0,len(a)):
    a[grade] = a[grade]**0.5*5
    print(a[grade],end=' ')
print(end='\n')
nav = sum(a)
print('新3次平均: ', end='')
print(nav)
